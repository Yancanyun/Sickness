<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.emenu.mapper.bar.BarContrastMapper">
    <select id ="queryLastBarContrast" resultMap="pw.BarContrast">
        SELECT * FROM t_bar_contrast
        ORDER BY id DESC
        LIMIT 1
    </select>

    <select id ="listAll" resultMap="pw.BarContrast">
        SELECT * FROM t_bar_contrast
        ORDER BY id DESC
    </select>
</mapper>